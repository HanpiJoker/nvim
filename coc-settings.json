{
	"vimlsp.indexes.count": 5,
	"vimlsp.indexes.gap": 50,
	"coc.preferences.extensionUpdateCheck": "daily",
	"suggest.enablePreview": true,
	"suggest.maxPreviewWidth": 50,
	"suggest.maxCompleteItemCount": 25,
	"suggest.labelMaxLength": 50,
	"suggest.detailMaxLength": 50,
	"suggest.detailField": "preview",
	"suggest.removeDuplicateItems": true,
	"suggest.minTriggerInputLength": 2,
	"suggest.acceptSuggestionOnCommitCharacter": true,
	"suggest.noselect": true,
	"suggest.triggerAfterInsertEnter": true,
	"suggest.enablePreselect": false,
	"suggest.completionItemKindLabels": {
		"class": "\uf0e8",
		"color": "\ue22b",
		"constant": "\uf8fe",
		"default": "\uf29c",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"event": "\ufacd",
		"field": "\uf93d",
		"file": "\uf723",
		"folder": "\uf115",
		"function": "\u0192",
		"interface": "\uf417",
		"keyword": "\uf1de",
		"method": "\uf6a6",
		"module": "\uf40d",
		"operator": "\uf915",
		"property": "\ue624",
		"reference": "\ufa46",
		"snippet": "\ue60b",
		"struct": "\ufb44",
		"text": "\ue612",
		"typeParameter": "\uf728",
		"unit": "\uf475",
		"value": "\uf89f",
		"variable": "\ue71b"
	},
	"diagnostic.errorSign": "\uf467",
	"diagnostic.warningSign": "\uf071",
	"diagnostic.infoSign": "\uf129",
	"diagnostic.hintSign": "\uf864",
	"diagnostic.displayByAle": false,
	"diagnostic.refreshOnInsertMode": false,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",
	"diagnostic.virtualText": true,
	"codeLens.enable": true,
	"coc.preferences.useQuickfixForLocations": true,
	//"coc.preferences.formatOnSaveFiletypes": [ "json", "c", "cpp" ],
	"coc.source.ultisnips.filetypes": [
		"tex"
	],
	"rust-analyzer.serverPath": "/usr/sbin/rust-analyzer",
	"rust-analyzer.hoverActions.enable": true,
	"rust-analyzer.hoverActions.linksInHover": true,
	"rust-analyzer.updates.channel": "nightly",
	"rust-analyzer.lens.methodReferences": true,
	"languageserver": {
		"ccls": {
			"command": "ccls",
			"filetypes": [
				"c",
				"cpp",
				"cuda",
				"objc",
				"objcpp"
			],
			"trace.server": "verbose",
			"rootPatterns": [
				".ccls",
				"compile_commands.json",
				".vim/",
				".git/",
				".hg/"
			],
			"initializationOptions": {
				"index": { "threads" : 6 },
				"client": { "snippetSupport": true },
				"cache": { "directory": "/tmp/ccls-cache" },
				"highlight": { "lsRanges" : true }
			}
		}
  },
  "git.enableGutters": true
}
